<div class="user_title"><h1> <%=@user.username%>'s Profile</h1>
	<% if user_signed_in? %>
		<% if current_user.username == @user.username %>
    	<%= link_to 'Edit Profile', edit_user_registration_path %>
    	<% end %>
    <%end %>
</div>

<div class="user_pics"><h2>Posted Photos</h2><br>
  <%@pics.each do |p|%>
      <%= link_to picture_path(p.id) do %>
  	  <%image_tag p.image.url(:box)%>
    <%end%>
  <%end%>
</div>
