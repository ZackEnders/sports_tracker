<h3>Create Picture</h3>
<div class="InputBox">
  <h2>New Picture:</h2>
<%=current_user%>

<%= form_for @pic do |f| %>
<%#= f.hidden_field :user_id, :value => @userid, :required => true %>
<%#= f.hidden_field :game_id, :value => @gameid, :required => true %>
<%= f.text_area :description %>
<%=f.file_field :image%>
<%= f.submit %>
<% end %>
</div>
